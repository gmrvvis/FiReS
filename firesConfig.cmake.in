# - Config file for the fires package
# It defines the following variables
#  FIRES_INCLUDE_DIRS - include directories for FooBar
#  FIRES_LIBRARIES    - libraries to link against
 
# Compute paths
get_filename_component(FIRES_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(FIRES_INCLUDE_DIRS "@INSTALL_INCLUDE_DIR@")
 
# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET fires AND NOT fires_BINARY_DIR)
  include("${FIRES_CMAKE_DIR}/firesTargets.cmake")
endif()
 
# These are IMPORTED targets created by firesTargets.cmake
set(FIRES_LIBRARIES fires)


message(STATUS "Using fires @fires_VERSION@ from @fires_ ")

